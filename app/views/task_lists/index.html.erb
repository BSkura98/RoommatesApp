<div class="card-header d-flex justify-content-between">
  <span>
    Task lists
  </span>
  <%= link_to 'New Task List', new_task_list_path, class: 'btn btn-primary btn-sm' %>

</div>

<ul class='list-group'>
  <% @task_lists.each do |task_list| %>
    <li class='list-group-item'>
      <%= link_to task_list.title, task_list %>
    </li>
  <% end %>
</ul>
